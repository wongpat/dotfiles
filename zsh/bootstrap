#!/bin/sh

BASE=$(dirname $0)
mkdir -p ~/.zsh
cd ~/.zsh 
git clone git://github.com/olivierverdier/zsh-git-prompt.git git-prompt 
git clone git://github.com/zsh-users/zsh-syntax-highlighting.git
cd -
cp $BASE/zshrc ~/.zshrc && echo "zshrc envionrment created. You can open a new zsh shell to see changes"
